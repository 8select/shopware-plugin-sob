           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="eightselect_c_s_e.article_export" class="EightselectCSE\Components\ArticleExport"> </service>
        <service id="eightselect_c_s_e.quick_update" class="EightselectCSE\Components\QuickUpdate"> </service>

        <service id="eightselect_c_s_e.emotion_view_subscriber" class="Shopware\Components\Emotion\EmotionComponentViewSubscriber">
