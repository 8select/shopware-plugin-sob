{
  "name": "8select/shopware-plugin-sob",
  "description": "Shopware plugin to use 8select CSE",
  "license": "proprietary",
  "type": "shopware-plugin",
  "extra": {
    "installer-name": "CseEightselectBasic"
  },
  "require": {
    "php": "^5.6.4"
  },
  "config": {
    "platform": {
      "php": "5.6.4"
    }
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.2",
    "wimg/php-compatibility": "^8.1",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4"
  },
  "scripts": {
    "php-comp":
      "phpcs -p . --standard=PHPCompatibility --ignore=vendor/* --runtime-set testVersion 5.6"
  }
}
